{
  "name": "contrast",
  "productName": "Contrast",
  "version": "0.0.1",
  "description": "A cross-platform diff tool",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest \"(\\\\|/)test(\\\\|/)(?!.*e2e\\.spec\\.js).+\\.spec\\.js\"",
    "test-all": "npm run lint && npm run flow && npm run test && npm run build",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest ./test/e2e.spec.js",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "npm run hot-server -- --start-hot",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "cleanup": "mop -v"
  },
  "browserslist": "electron 1.4",
  "build": {
    "productName": "Contrast",
    "appId": "org.amilajack.Contrast",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "category": "Utility",
      "target": [
        "deb",
        "snap",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/contrast.git"
  },
  "author": {
    "name": "Amila Welihinda",
    "email": "amilajack@gmail.com",
    "url": "https://github.com/amilajack"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amilajack/contrast/issues"
  },
  "keywords": [
    "electron",
    "react",
    "diff",
    "tool",
    "difftool"
  ],
  "homepage": "https://github.com/amilajack/contrast#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "asar": "^0.14.3",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "boiler-room-custodian": "^0.6.1",
    "coffee-loader": "^0.9.0",
    "concurrently": "^3.4.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "devtron": "^1.4.0",
    "diff": "^3.2.0",
    "electron": "^2.0.7",
    "electron-builder": "^20.28.1",
    "electron-devtools-installer": "^2.1.0",
    "electron-rebuild": "^1.5.7",
    "enzyme": "^3.4.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.15.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fbjs-scripts": "^0.8.3",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.78.0",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.0.0",
    "highlights": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jison-loader": "^1.0.0",
    "jquery": "^3.1.1",
    "jsdom": "^11.12.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-loader": "^0.6.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^16.4.2",
    "redux-logger": "^3.0.6",
    "sinon": "^6.1.5",
    "spectron": "^3.6.0",
    "style-loader": "^0.22.1",
    "url-loader": "^1.1.0",
    "webpack": "^4.16.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.1.4",
    "webpack-validator": "^3.0.1"
  },
  "dependencies": {
    "electron-debug": "^2.0.0",
    "font-awesome": "^4.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "source-map-support": "^0.5.8"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
